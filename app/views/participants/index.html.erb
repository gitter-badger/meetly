<h1>Lista uczestników</h1>
<table>
  <tr>
    <th>Imie</th>
    <th>Nazwisko</th>
    <th>Wiek</th>
    <th>Miasto</th>
    <th>E-mail</th>
    <th>Telefon</th>
    <th>Rola</th>
    <th>Koszt</th>
    <th>Zapłacono</th>
    <th>Ilosc dni</th>
    <th>Ilosc noclegow</th>
    <th>Ilosc obiadow</th>
    <th>Utworzony</th>
  </tr>

<% @participants.each do |participant| %>
<tr>
  <td><%= participant.name%></td>
  <td><%=participant.surname%></td>
  <td><%=participant.age%></td>
  <td><%=participant.city%></td>
  <td><%=participant.email%></td>
  <td><%=participant.phone%></td>
  <td><%=participant.role.name%></td>
  <td><%=participant.cost%></td>
  <td><%=participant.paid%></td>
  <td><%=participant.days.length%></td>
  <td><%=participant.nights.length%></td>
  <td><%=participant.dinners.length%></td>
  <td><%= participant.created_at%></td>
</tr>




<%end%>
</table>